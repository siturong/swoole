{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "acquia/drupal-environment-detector": "^1.4",
        "aferrandini/phpqrcode": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/conditional_fields": "4.0.0-alpha1",
        "drupal/config_devel": "^1.8",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/cshs": "^3.3",
        "drupal/decoupled_pages": "^1.0",
        "drupal/devel": "^4.1",
        "drupal/drush_language": "1.0-rc4",
        "drupal/extlink": "^1.6",
        "drupal/field_group": "^3.1",
        "drupal/formtips": "^1.5",
        "drupal/group": "^1.3",
        "drupal/key_auth": "^1.1",
        "drupal/memcache": "^2.3",
        "drupal/migrate_devel": "2.0-alpha2",
        "drupal/migrate_tools": "5.x-dev@dev",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/multivalue_form_element": "^1.0.0-beta2",
        "drupal/paragraphs": "dev-1.x",
        "drupal/password_policy": "^3.0",
        "drupal/require_login": "^2.4",
        "drupal/restui": "^1.20",
        "drupal/structure_sync": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drush/drush": "^10.3",
        "league/uri": "^6.4",
        "league/uri-components": "^2.3",
        "phpoffice/phpspreadsheet": "^1.18",
        "sainsburys/guzzle-oauth2-plugin": "^3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/paragraphs": {
                "[migrate_upgrade compatibility] d7_field_instance upgrade fails due to redundant 'field_' prefix removal on bundle": "https://www.drupal.org/files/issues/2021-01-29/paragraphs-n3192993-5.patch",
                "Derive Field Collection and Paragraph entity migrations based on their parent entity type ID (and parent entity bundle)": "https://www.drupal.org/files/issues/2021-03-29/paragraphs-derive_by_parent-3145755-23--complete--mr-5-700e9d11.diff",
                "Paragraphs migrations broken in Drupal >=9.1.4": "https://www.drupal.org/files/issues/2021-03-29/3203739-6.patch",
                "[PP-4] Multifield to Paragraphs migration": "https://www.drupal.org/files/issues/2021-06-28/paragraphs-multifield_to_paragraphs-2977853-45--core-9.1-9.2--complete.patch"
            },
            "drupal/core": {
                "Fix multifiled migrate issue": "./patches/migrate-drupal-fix-multifiled-issue.patch",
                "Call to undefined function .... in options_allowed_values()": "https://www.drupal.org/files/issues/2020-07-03/gracefully_handle_call_to_undefined_func-3156608-1.patch"
            },
            "drupal/migrate_plus": {
                "Fix migrate plus http & oauth2 plugin": "./patches/migrate_plus_http_oauth2_plugin.patch"
            },
            "drupal/scheduler": {
                "migration handler": "https://www.drupal.org/files/issues/2021-07-08/scheduler-migrate-drupal-3012154-9.patch"
            },
			"drupal/field_group": {
            	"fix validate js issue": "https://www.drupal.org/files/issues/2019-08-22/2787179-highlight-html5-validation-40.patch",
				"fix document tabs focus": "./patches/doc_tab_init.patch"
        	}
        }
    }
}
